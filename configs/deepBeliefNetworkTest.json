{
    "name": "deep_belief_network",
    "model": {
        "type": "DBN",
        "args": {
            "n_visible": 49,
            "n_hidden": [64],
            "n_classes": 6,
            "learning_rate": [0.1],
            "momentum": [0.8],
            "decay": [0.1],
            "batch_size": [64],
            "num_epochs": [1],
            "k": [1]
        }
    },
    "data_loader": {
        "type": "CICIDS2017DataLoader",
        "args": {
            "batch_size": 128,
            "balanced": false,
            "knownAttacksGrouped": false 
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": false
        }
    },
    "loss": {
        "type": "CrossEntropyLoss",
        "args": {
            "reduction": "mean"
        }
    },
    "trainer": {
        "num_epochs": 1
    }
}