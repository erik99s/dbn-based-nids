{
    "name": "deep_belief_network",
    "model": {
        "type": "DBN",
        "args": {
            "n_visible": 49,
            "n_hidden": [81, 42, 26, 13],
            "n_classes": 6,
            "learning_rate": [0.01, 0.01, 0.01, 0.01],
            "momentum": [0.9, 0.9, 0.9, 0.9],
            "decay": [0, 0, 0, 0],
            "batch_size": [64, 64, 64, 64],
            "num_epochs": [10,10,10,10],
            "k": [1, 1, 1, 1]
        }
    },
    "auto_encoder_1st": {
        "type": "AE_1st",
        "args": {
            "n_visible": 49,
            "n_hidden": [256,128],
            "n_classes": 64,
            "learning_rate": 1e-3,
            "batch_size": 64,
            "num_epochs": 70
        }
    },
    "auto_encoder_2nd":{
        "type": "AE_2nd",
        "args": {
            "n_visible": 49,
            "n_hidden": [256,128],
            "n_classes": 64,
            "learning_rate": 1e-3,
            "batch_size": 64,
            "num_epochs": 70
        } 
    },
    "data_loader": {
        "type": "CICIDS2017DataLoader",
        "args": {
            "batch_size": 128,
            "balanced": false
        }
    },
    "data_loader_ae": {
        "type": "CICIDS2017DataLoader",
        "args": {
            "batch_size": 64
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.001,
            "weight_decay": 0.001,
            "amsgrad": false
        }
    }, 
    "loss": {
        "type": "CrossEntropyLoss",
        "args": {
            "reduction": "mean"
        }
    }, 
    "lossAE": {
        "type": "MSELoss",
        "args": {
            "reduction": "mean"
        }
    },
    "trainer": {
        "num_epochs": 3
    }

}