{
    "name": "deep_belief_network",
    "model": {
        "type": "DBN",
        "args": {
            "n_visible": 49,
            "n_hidden": [81, 42, 26, 13],
            "n_classes": 5,
            "learning_rate": [0.01, 0.01, 0.01, 0.01],
            "momentum": [0.9, 0.9, 0.9, 0.9],
            "decay": [0, 0, 0, 0],
            "batch_size": [64, 64, 64, 64],
            "num_epochs": [5,5,5,5],
            "k": [1, 1, 1, 1]
        }
    },
    "auto_encoder": {
        "type": "AE",
        "args": {
            "n_visible": 49,
            "n_hidden": [128,64,32,16],
            "n_classes": 2,
            "learning_rate": 1e-3,
            "batch_size": 64,
            "num_epochs": 10
        }
    },
    "data_loader": {
        "type": "CICIDS2017DataLoader",
        "args": {
            "batch_size": 128,
            "balanced": false,
            "knownAttacksGrouped": false 
        }
    },
    "data_loader_ae": {
        "type": "CICIDS2017DataLoader",
        "args": {
            "batch_size": 128
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": false
        }
    }, 
    "loss": {
        "type": "CrossEntropyLoss",
        "args": {
            "reduction": "mean"
        }
    }, 
    "lossAE": {
        "type": "MSELoss",
        "args": {
            "reduction": "mean"
        }
    },
    "trainer": {
        "num_epochs": 5
    }

}